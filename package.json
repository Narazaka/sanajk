{
  "name": "sanajk",
  "version": "0.1.1",
  "description": "Ukagaka SHIORI subsystem 'SanaJK'",
  "keywords": [],
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "gen"
  ],
  "scripts": {
    "prepare": "npm run dist",
    "prepublishOnly": "npm run lint && npm run test",
    "dist": "shx rm -rf dist && tsc",
    "clean": "shx rm -rf lib \"test/**/*.{ts,js,map}\"",
    "test": "mocha",
    "lint": "tslint -p .",
    "docdist": "cd doc && git clean -fd && git rm -rf * && cd .. && npm run doc && cd doc && touch .nojekyll && git add . && git commit -m doc && git push",
    "doc": "typedoc --mode modules --out doc --readme Readme.md --disableOutputCheck src && cd doc && git status && cd .."
  },
  "dependencies": {
    "debug": "^4.0.0",
    "fso": "^2.5.15",
    "shiorack": "^2.2.0",
    "shiori-request-helper": "^2.3.0",
    "shiorijk": "^1.0.1"
  },
  "devDependencies": {
    "@types/debug": "0.0.30",
    "@types/mocha": "^5.0.0",
    "@types/node": "^10.0.3",
    "@types/power-assert": "^1.4.29",
    "espower-typescript": "^9.0.0",
    "mocha": "^5.0.0",
    "power-assert": "^1.4.4",
    "shx": "^0.3.0",
    "tslint": "^5.9.1",
    "typedoc": "^0.12.0",
    "typescript": "^2.7.1"
  },
  "url": "https://github.com/Narazaka/sanajk",
  "author": "Narazaka",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Narazaka/sanajk.git"
  }
}
